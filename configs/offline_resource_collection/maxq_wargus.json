{
  "environment": "../src/environment/maxq_wargus_small.json",
  "agentsystem": {
    "name": "HierarchicalSystem"
  },
  "policy": {
    "name": "TabularPolicy",
    "policy_sampler": {
      "name": "DiscreteEGreedy",
      "epsilon": 0.05
    }
  },
  "algorithm": {
    "name": "MaxQ",
    "is_online": true,
    "learning_rate": 0.25,
    "discount_factor": 1.0
  },
  "callbacks": [
    {"name": "Evaluate", "eval_num": 10, "timestep": 2, "visualize": false},
    {"name": "DispResults"}
  ],
  "episodes": 750,
  "episode_max_length": 1000
}
