{
  "environment": {
    "name": "GridWorld",
    "tile_types": {
        "0": "EMPTY",
        "1": "WALL",
        "2": {"reward": 10, "terminate": true},
        "3": {"reward": -10, "terminate": true}
    },
    "map": [
      "0002",
      "0103",
      "0000"
    ],
    "agent_spawn": [[2,0]],
    "default_reward": -1,
    "collide_reward": -2,
    "feature_type": "absolute"
  },
  "agentsystem": {
    "name": "IndependentSystem"
  },
  "policy": {
    "name": "TabularPolicy",
    "policy_sampler": {
      "name": "DiscreteEGreedy",
      "epsilon": 0.2
    }
  },
  "algorithm": {
    "name": "SARSA",
    "is_online": true,
    "learning_rate": 0.01,
    "discount_factor": 0.95,
    "memory_size": 10000,
    "batch_size": 128,
    "update_interval": 1
  },
  "callbacks": [
    {"name": "Evaluate", "eval_num": 1, "timestep": 25, "visualize": false},
    {"name": "PlotReward"}
  ],
  "episodes": 1200,
  "episode_max_length": 300
}
