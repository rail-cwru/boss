{
  "environment": {
    "name" : "MountainCar"
  },
  "agentsystem": {
    "name": "IndependentSystem"
  },
  "policy": {
    "name": "FuncApproxPolicy",
    "policy_sampler": {
      "name": "DiscreteBoltzmann",
      "temperature": 1.0
    },
    "function_approximator": {
      "name": "DenseNeuralNet",
      "structure": [
        [24, "relu"],
        [24, "relu"]
      ],
      "optimizer": "adam"
    }
  },
  "algorithm": {
    "name": "Reinforce",
    "learning_rate": 0.01,
    "discount_factor": 0.99,
    "memory_size": 100,
    "batch_size": 10,
    "update_interval": 1
  },
  "callbacks": [
    {"name": "Evaluate", "eval_num": 10, "timestep": 1, "visualize": false}
  ],
  "episodes": 40000,
  "episode_max_length": 200
}
