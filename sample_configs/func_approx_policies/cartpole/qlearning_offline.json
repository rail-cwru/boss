{
  "environment": {
    "name" : "Cartpole"
  },
  "agentsystem": {
    "name": "IndependentSystem"
  },
  "policy": {
    "name": "FuncApproxPolicy",
    "policy_sampler": {
      "name": "DiscreteEGreedy",
      "epsilon": 1.0,
      "decay": 0.99,
      "min_epsilon": 0.01
    },
    "function_approximator": {
      "name": "DenseNeuralNet",
      "structure": [
        [24, "relu"],
        [24, "relu"]
      ],
      "optimizer": "adam"
    }
  },
  "algorithm": {
    "name": "QLearning",
    "is_online": false,
    "learning_rate": 0.01,
    "discount_factor": 0.95,
    "memory_size": 100,
    "batch_size": 10,
    "update_interval": 1
  },
  "callbacks": [
    {"name": "Evaluate", "eval_num": 10, "timestep": 25, "visualize": false},
    {"name": "PlotReward"}
  ],
  "episodes": 1200,
  "episode_max_length": 200
}
