{
  "environment": {
    "name" : "Cartpole"
  },
  "agentsystem": {
    "name": "IndependentSystem"
  },
  "policy": {
    "name": "FuncApproxPolicy",
    "policy_sampler": {
      "name": "DiscreteBoltzmann",
      "temperature": 1.0
    },
    "function_approximator": {
      "name": "Linear",
      "optimizer": "adam",
      "use_bias": true
    }
  },
  "algorithm": {
    "name": "Reinforce",
    "learning_rate": 0.025,
    "discount_factor": 0.65
  },
  "callbacks": [
    {"name": "Evaluate", "eval_num": 10, "timestep": 1, "visualize": false},
    {"name": "PlotReward"}
  ],
  "seed": 5,
  "episodes": 500,
  "episode_max_length": 500
}
