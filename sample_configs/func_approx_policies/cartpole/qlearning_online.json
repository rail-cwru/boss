{
  "environment": {
    "name" : "Cartpole"
  },
  "agentsystem": {
    "name": "IndependentSystem"
  },
  "policy": {
    "name": "FuncApproxPolicy",
    "policy_sampler": {
      "name": "DiscreteEGreedy",
      "epsilon": 0.2
    },
    "function_approximator": {
      "name": "DenseNeuralNet",
      "structure": [
        [24, "relu"],
        [24, "relu"]
      ],
      "optimizer": "adam"
    }
  },
  "algorithm": {
    "name": "QLearning",
    "is_online": true,
    "learning_rate": 0.01,
    "discount_factor": 0.95,
    "memory_size": 1000,
    "batch_size": 128,
    "update_interval": 1
  },
  "callbacks": [
    {"name": "Evaluate", "eval_num": 10, "timestep": 25, "visualize": false},
    {"name": "PlotReward"}
  ],
  "episodes": 1000,
  "episode_max_length": 200
}
