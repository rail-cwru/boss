{
  "environment": {
    "name" : "BipedalWalker"
  },
  "agentsystem": {
    "name": "IndependentSystem"
  },
  "policy": {
    "name": "FuncApproxPolicy",
    "policy_sampler": {
      "name": "DiscreteBoltzmann",
      "temperature": 1.0
    },
    "function_approximator": {
      "name": "DenseNeuralNet",
      "structure": [
        [256, "relu"],
        [256, "relu"],
        [128, "relu"]
      ],
      "optimizer": "adam"
    }
  },
  "algorithm": {
    "name": "Reinforce",
    "learning_rate": 0.01,
    "discount_factor": 0.99
  },
  "callbacks": [
    {
      "name": "Evaluate", 
      "eval_num": 10,
      "timestep": 1,
      "visualize": false
    }
  ],
  "episodes": 1,
  "episode_max_length": 1000,
  "seed": 2
}
